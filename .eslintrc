{
    "env": {
        "browser": true
    },
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "rules": {
        "camelcase": "error",
        "react/prop-types": "off",
        "no-invalid-this": "error"
    },
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": "latest"
    }
}